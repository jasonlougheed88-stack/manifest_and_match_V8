        <attribute name="githubURL" optional="YES" attributeType="String"/>
        <attribute name="professionalSummary" optional="YES" attributeType="String"/>

        <!-- Skills (Phase 1.75 - O*NET Categorized Skills Array) -->
        <attribute name="skills" attributeType="Transformable" valueTransformerName="NSSecureUnarchiveFromData" defaultValueString="[]"/>

        <!-- Career Stage -->
        <attribute name="currentDomain" attributeType="String" defaultValueString="technology"/>
        <attribute name="experienceLevel" attributeType="String" defaultValueString="mid"/>

        <!-- Job Preferences -->
        <attribute name="desiredRoles" attributeType="Transformable" valueTransformerName="NSSecureUnarchiveFromDataTransformer"/>
        <attribute name="locations" attributeType="Transformable" valueTransformerName="NSSecureUnarchiveFromDataTransformer"/>
        <attribute name="primaryLocation" optional="YES" attributeType="String"/>
        <attribute name="searchRadius" attributeType="Double" defaultValueString="50.0" usesScalarValueType="YES"/>
        <attribute name="remotePreference" attributeType="String"/>
        <attribute name="salaryMin" optional="YES" attributeType="Integer 32" defaultValueString="0"/>
        <attribute name="salaryMax" optional="YES" attributeType="Integer 32" defaultValueString="0"/>

        <!-- Dual-Profile System -->
        <attribute name="amberTealPosition" attributeType="Double" defaultValueString="0.5" usesScalarValueType="YES"/>

        <!-- AI Configuration Sliders (Phase 1 - AI Slider Fix) -->
        <attribute name="explorationRate" attributeType="Double" defaultValueString="0.3" usesScalarValueType="YES"/>
        <attribute name="confidenceThreshold" attributeType="Double" defaultValueString="0.7" usesScalarValueType="YES"/>

        <!-- O*NET Profile Fields (Phase 2 - October 31, 2025) -->
        <!-- Education Level: O*NET 1-12 scale (1=Less than HS, 8=Bachelor's, 12=Doctoral) -->
        <attribute name="onetEducationLevel" optional="YES" attributeType="Integer 16" defaultValueString="0" usesScalarValueType="YES"/>

        <!-- Work Activities: Dictionary of 41 O*NET activities with importance scores (1-7) -->
        <!-- Format: ["4.A.1.a.1": 5.0, "4.A.2.a.1": 6.5, ...] -->
        <attribute name="onetWorkActivities" optional="YES" attributeType="Transformable" valueTransformerName="NSSecureUnarchiveFromData"/>

        <!-- RIASEC Interest Profile: Holland Codes personality dimensions (0.0-7.0 each) -->
        <!-- R = Realistic (hands-on, mechanical) -->
        <attribute name="onetRIASECRealistic" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- I = Investigative (analytical, scientific) -->
        <attribute name="onetRIASECInvestigative" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- A = Artistic (creative, expressive) -->
        <attribute name="onetRIASECArtistic" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- S = Social (helping, teaching) -->
        <attribute name="onetRIASECSocial" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- E = Enterprising (persuading, leading) -->
        <attribute name="onetRIASECEnterprising" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- C = Conventional (organized, detail-oriented) -->
        <attribute name="onetRIASECConventional" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>

        <!-- Work Styles: O*NET work style dimensions (1.0-5.0 each) - HOW user approaches work -->
        <!-- Achievement Orientation: persistence, initiative, goal-setting -->
        <attribute name="onetWorkStyleAchievement" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- Social Influence: leadership, persuasion, inspiring others -->
        <attribute name="onetWorkStyleSocialInfluence" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- Interpersonal Orientation: teamwork, helping, caring for others -->
        <attribute name="onetWorkStyleInterpersonal" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- Adjustment: stress tolerance, self-control, adaptability -->
        <attribute name="onetWorkStyleAdjustment" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- Conscientiousness: dependability, attention to detail, integrity -->
        <attribute name="onetWorkStyleConscientiousness" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- Independence: autonomy, preference for individual responsibility -->
        <attribute name="onetWorkStyleIndependence" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>
        <!-- Practical Intelligence: innovation, analytical thinking -->
        <attribute name="onetWorkStylePracticalIntelligence" optional="YES" attributeType="Double" defaultValueString="0.0" usesScalarValueType="YES"/>

        <!-- Relationships (Cascade Delete) -->
        <relationship name="workExperience" optional="YES" toMany="YES" deletionRule="Cascade" destinationEntity="WorkExperience" inverseName="profile" inverseEntity="WorkExperience"/>
        <relationship name="education" optional="YES" toMany="YES" deletionRule="Cascade" destinationEntity="Education" inverseName="profile" inverseEntity="Education"/>
        <relationship name="certifications" optional="YES" toMany="YES" deletionRule="Cascade" destinationEntity="Certification" inverseName="profile" inverseEntity="Certification"/>
